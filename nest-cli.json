{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "schematics": {
    "@nestjs/schematics:application": {
      "unitTestRunner": "none",
      "e2eTestRunner": "none"
    },
    "@nestjs/schematics:library": {
      "unitTestRunner": "none",
      "e2eTestRunner": "none"
    }
  }
}
